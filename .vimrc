autocmd vimenter * colorscheme gruvbox
set number
set nocompatible
syntax on
filetype indent plugin on
set wildmenu
set showcmd
set ignorecase
set smartcase
set backspace=indent,eol,start
set autoindent
set ruler
set laststatus=2
set confirm
set visualbell
set mouse=a
set cmdheight=2
set notimeout ttimeout ttimeoutlen=200
set shiftwidth=4
set softtabstop=4
set expandtab
let g:airline_theme='base16_gruvbox_dark_hard'
